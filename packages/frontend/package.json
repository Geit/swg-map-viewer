{
  "name": "@swg-map-viewer/frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@juggle/resize-observer": "3.3.1",
    "@material-ui/core": "4.11.4",
    "@material-ui/lab": "4.0.0-alpha.58",
    "drei": "2.2.21",
    "leaflet": "1.7.1",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-copy-to-clipboard": "5.0.3",
    "react-dom": "17.0.2",
    "react-leaflet": "2.8.0",
    "react-router-dom": "5.2.0",
    "react-three-fiber": "5.3.22",
    "react-use-measure": "2.0.4",
    "recoil": "0.3.1",
    "three": "0.124.0"
  },
  "devDependencies": {
    "@svgr/parcel-plugin-svgr": "^5.5.0",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.6.4",
    "@types/react": "^17.0.0",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-leaflet": "^2.5.2",
    "@types/react-router-dom": "^5.1.5",
    "@types/rimraf": "^3.0.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "sass": "^1.35.2"
  },
  "scripts": {
    "start": "parcel ./src/index.html -d build",
    "build": "rimraf build && parcel build ./src/index.html -d build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "generate-tiles": "yarn ts-node-script scripts/generate-tiles.ts"
  },
  "browserslist": {
    "production": [
      ">1.0%",
      "not ie 11",
      "not dead",
      "not op_mini all",
      "not safari 5.1"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  }
}
